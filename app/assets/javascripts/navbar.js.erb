$(document).ready(function() {

  $("#navbar ul li.navbar_drop a").live('mouseover', function() {
    $(this).parent().find(".navbar_drop_child").delay(100).show(50);
  });
  
  $("#navbar ul li.navbar_drop").live('mouseleave', function() {
    $(this).find(".navbar_drop_child").stop(1).hide(50);
  });
  
});