html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td {
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  font-size: 12px;
  vertical-align: baseline;
  background: transparent;
}
html, body {
  margin: 0;
  margin-bottom: 25px;
  padding: 0;
  background-color: #eee;
  font-family: "Trebuchet MS", "Helvetica Neue", "Arial", sans-serif;
  font-size: 12px;
  color: #666;
  height: 100%;
  line-height: 140%;
  background-image:url(<%=image_path('background-gradient.png')%>);
  background-repeat:repeat-x;
  background-size:5px 160px;
}
h1 {
  font-size: 16px;
  line-height: 2.0;
  padding:0 15px 0 15px;
  margin: 0px 0px 10px 0px;
  text-align: center;  
  white-space: nowrap;
  overflow: hidden;
  border-bottom: 2px solid #eee;
  height:30px;
  
  text-shadow: 0 1px 0px #EEE;
  border-top-right-radius: 10px;
  -moz-border-radius-topright: 10px;
  -webkit-border-top-right-radius: 10px;
  border-top-left-radius: 10px;
  -moz-border-radius-topleft: 10px;
  -webkit-border-top-left-radius: 10px;
  background: #edf1f2; /* Old browsers */
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-linear-gradient(-45deg,  #edf1f2 19%, #e5e9ea 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, right bottom, color-stop(19%,#edf1f2), color-stop(100%,#e5e9ea)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* IE10+ */
  background: linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#edf1f2', endColorstr='#e5e9ea',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */

}
h2 {
  width:auto;
  font-size: 14px;
  line-height: 1.5;
  margin: 15px 0 25px 0;
  text-shadow: 0 1px 0px #EEE;
  display:block;
  border-bottom: 1px solid #eee;
}
h3 {
  width:auto;
  font-size: 13px;
  line-height: 1.25;
  margin: 10px 0 10px 0;
  text-shadow: 0 1px 0px #EEE;
  display:block;
  text-decoration: underline;
}
ul, ol {
  list-style-position: inside;
}
ul.indent{
  list-style-position: outside;
  margin-left:20px;
}
p {
  word-wrap: break-word;
}
a {
  color: blue;
  text-decoration: none;
}
a:hover, a:focus {
	text-decoration: underline;
}
select, input, .button, input[type="text"], input[type="search"], textarea  {
  outline:none;
  border: 1px solid #aaa;
  color: #666;
  padding: 3px 4px;
  font-size: 12px;
  font-family: "Trebuchet MS", "Helvetica Neue", "Arial", sans-serif;
  -moz-border-radius: 8px;
  -webkit-border-radius: 8px;
  border-radius: 8px;
  -moz-box-shadow:    inset 0px 1px 4px 1px #ddd;
  -webkit-box-shadow: inset 0px 1px 4px 1px #ddd;
  box-shadow:         0px -1px 0px 0px #999 , inset 0px 1px 4px 0px #ddd;
}
select[disabled] {
 color: #bbb;
}

/* clearfix */
.group:before, .group:after {
    content:"";
    display:table;
}
.group:after {
    clear:both;
}
.group {
    zoom:1; /* For IE 6/7 (trigger hasLayout) */
}
#site {
  width: 1000px;
  margin: auto;
  position: relative;
}
#header {
  margin: 0 auto;
  width: 100%;
  min-height: 40px;
}
#followus{
	float:right;
	margin: 5px 5px 0 0 ;
}
#logobox {
  position:absolute;
  left:25px;
  top:45px;
}
#logo {
  float: left;
}
#logo img {
  height: 70px;
  width: 155px;
}
#logo a {
  text-decoration: none;
}
.button, input[type="submit"] {
  -moz-box-shadow:    0 0 0 0 #FFF;
  -webkit-box-shadow: 0 0 0 0 #FFF;
  box-shadow:         0 0 0 0 #FFF;
  font-size: 100%;
  padding: 3px 7px 3px 7px;
  margin:0px;
  color: #fff;
  display: inline-block;
  text-decoration: none;
  text-shadow: 0 -1px 0px rgb(100,100,100);
  background: #ABB0B1;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  background: rgb(192,192,192); /* Old browsers */
  background: -moz-linear-gradient(top,  rgba(192,192,192,1) 0%, rgba(176,176,176,1) 50%, rgba(159,159,159,1) 51%, rgba(112,112,112,1) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(192,192,192,1)), color-stop(50%,rgba(176,176,176,1)), color-stop(51%,rgba(159,159,159,1)), color-stop(100%,rgba(112,112,112,1))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  rgba(192,192,192,1) 0%,rgba(176,176,176,1) 50%,rgba(159,159,159,1) 51%,rgba(112,112,112,1) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  rgba(192,192,192,1) 0%,rgba(176,176,176,1) 50%,rgba(159,159,159,1) 51%,rgba(112,112,112,1) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  rgba(192,192,192,1) 0%,rgba(176,176,176,1) 50%,rgba(159,159,159,1) 51%,rgba(112,112,112,1) 100%); /* IE10+ */
  background: linear-gradient(top,  rgba(192,192,192,1) 0%,rgba(176,176,176,1) 50%,rgba(159,159,159,1) 51%,rgba(112,112,112,1) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c0c0c0', endColorstr='#707070',GradientType=0 ); /* IE6-9 */
}
.button:hover, .button:focus, input[type="submit"]:hover, .active_button {
  color: #fff;
  cursor: pointer;
  background: #BEC1C4;
  text-decoration: none;
  text-shadow: none;
}
#topnav {
  padding: 5px 15px 5px 15px;
  -webkit-border-bottom-right-radius: 5px;
  -webkit-border-bottom-left-radius: 5px;
  -moz-border-radius-bottomright: 5px;
  -moz-border-radius-bottomleft: 5px;
  border-bottom-right-radius: 5px;
  border-bottom-left-radius: 5px;
}
#topnav a {
  padding: 1px 5px;
}
#navbar {
  width: 80%;
  margin: 0;
  float: right;
  height: 40px;
  line-height: 40px;
  background: #ABB0B1;
  -moz-border-radius: 0px 10px 0px 35px;
  border-radius: 0px 10px 0px 35px;
  -moz-box-shadow:    0px 1px 1px 0px rgba(106,108,110,0.8);
  -webkit-box-shadow: 0px 1px 1px 0px rgba(106,108,110,0.8);
  box-shadow:         0px 1px 1px 0px rgba(106,108,110,0.8);
}
#navbar ul {
  list-style: none; 
}
#navbar ul li {
  float: right;
  display: inline;
  text-align: center;
  border-left: 1px solid #DDD ;
}
#navbar ul li a {
  padding: 0 20px;
  text-decoration: none;
  color: #fff;
  display: block;
  text-transform: capitalize;
  letter-spacing: 1px;
  font-size: 100%;
  text-shadow: 0 -1px 0px rgb(100,100,100);
}
#navbar ul li a:hover, #navbar ul li a:focus, #navbar ul li a:active {
  color: #fff;
  background: #BEC1C4;
  text-decoration: none;
  text-shadow: 0 -1px 0px rgb(100,100,100);
  cursor: pointer;
}
#navbar ul li.navbar_drop {
  position:relative;
}
/*#navbar ul li.navbar_drop a {
  padding-right:27px;
  background: url(<%= asset_path('chosen-sprite.png') %>) no-repeat 70px 6px;
  background-size:80%;
}*/
#navbar ul li.navbar_drop a:hover, #navbar ul li.navbar_drop a:focus, #navbar ul li.navbar_drop a:active  {
  background-color: #BEC1C4;
  text-decoration: none;
  text-shadow: 0 -1px 0px rgb(100,100,100);
  cursor: pointer;
}
#navbar ul li.navbar_drop div.navbar_drop_child {
   display:none;
   z-index:1000;
   position:absolute;
   top:20px;
   right:0px;
   margin-top:20px;
   background: #fff;
   border: 1px solid #ABB0B1;
   color: #666;
   -moz-border-radius: 0px 0px 8px 8px;
   border-radius:      0px 0px 8px 8px;
   -moz-box-shadow:    0px 1px 3px 0px rgba(106,108,110,0.8);
   -webkit-box-shadow: 0px 1px 3px 0px rgba(106,108,110,0.8);
   box-shadow:         0px 1px 3px 0px rgba(106,108,110,0.8);   
}
#navbar ul li.navbar_drop div.navbar_drop_child ul {
  list-style: none;     
  min-width: 150px;
}
#navbar ul li.navbar_drop div.navbar_drop_child ul li {

  white-space: nowrap;
  display:block;
  width:100%;
  border-bottom: 1px solid #ddd;
  line-height:2;
  text-align: left;   
}
#navbar ul li.navbar_drop div.navbar_drop_child ul li.heading {
  font-weight:bold;
  text-indent:10px;
  background:#eee;
}
#navbar ul li.navbar_drop div.navbar_drop_child ul li a {  
  background: #fff;
  text-decoration: none;
  text-shadow: none;
  letter-spacing: 0;
  border: none;
  font-size: 100%;
  clear: both;  
  color: #666;
  margin:2px;
  padding: 0px 10px;
  border: 1px solid #fff;
}
#navbar ul li.navbar_drop div.navbar_drop_child ul li a:hover, #navbar ul li.navbar_drop div.navbar_drop_child ul li a:focus, #navbar ul li.navbar_drop div.navbar_drop_child ul li a:active {
  cursor: pointer;
  border: 1px solid #999999; background: #dadada url(<%= asset_path("ui-bg_glass_75_dadada_1x400.png") %>) 50% 50% repeat-x; color: #212121;
  border-radius:5px;
}

.round_left {
  border-radius: 10px 0 0 10px;
}
.round_right {
  border-top-right-radius: 10px;
  -moz-border-radius-topright: 10px;
  -webkit-border-top-right-radius: 10px;
}
#main {
  width: 100%;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  -moz-box-shadow:    0px 2px 15px 0px #333;
  -webkit-box-shadow: 0px 2px 15px 0px #333;
  box-shadow:         0px 2px 15px 0px #333;
  margin-bottom: 20px;
  padding-bottom: 20px;
  background-color: #CFD5D7;
}
