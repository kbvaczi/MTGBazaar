html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td {
  margin: 0;
  padding: 0;
  border: 0;
  outline: 0;
  font-size: 12px;
  vertical-align: baseline;
  background: transparent;
}
html, body {
  margin: 0;
  margin-bottom: 25px;
  padding: 0;
  background-color: #eee;
  font-family: "Trebuchet MS", "Helvetica Neue", "Arial", sans-serif;
  font-size: 12px;
  color: #555;
  height: 100%;
  line-height: 140%;
  background-image:url(<%=image_path('background-gradient.png')%>);
  background-repeat:repeat-x;
  background-size:5px 160px;
}

h1 {
  font-size: 12px;
  line-height: 2.4;
  padding:0 10px 0 10px;
  margin: 0px 0px 10px 0px;
  text-align: center;  
  white-space: nowrap;
  overflow: hidden;
  border-bottom: 2px solid #eee;
  height:25px;
  text-shadow: 0px 1px white;
  border-top-right-radius: 10px;
  -moz-border-radius-topright: 10px;
  -webkit-border-top-right-radius: 10px;
  border-top-left-radius: 10px;
  -moz-border-radius-topleft: 10px;
  -webkit-border-top-left-radius: 10px;
  background: #edf1f2; /* Old browsers */
  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
  background: url(data:image/svg+xml;base64,);
  background: -moz-linear-gradient(-45deg,  #edf1f2 19%, #e5e9ea 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, right bottom, color-stop(19%,#edf1f2), color-stop(100%,#e5e9ea)); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* IE10+ */
  background: linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#edf1f2', endColorstr='#e5e9ea',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */

}
h2 {
  font-size: 14px;
  line-height: 2;
  margin: 15px -5px 25px -5px;
  padding:0 10px;
  text-shadow: 0 1px 0px #EEE;
  display:block;
  border-bottom: 1px solid #eee;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
}
h3 {
  width:auto;
  font-size: 13px;
  line-height: 1.25;
  margin: 10px 0 10px 0;
  text-shadow: 0 1px 0px #EEE;
  display:block;
  text-decoration: underline;
}

h4 {
	font-size: 12px;
	  line-height: 2.4;
	  padding:0 15px 0 15px;
	  margin: 0px 0px 10px 0px;
	  text-align: center;  
	  white-space: nowrap;
	  overflow: hidden;
	  border-bottom: 2px solid #eee;
	  height:25px;
	  text-shadow: 0px 1px white;
	  border-top-right-radius: 0px;
	  -moz-border-radius-topright: 0px;
	  -webkit-border-top-right-radius: 0px;
	  border-top-left-radius: 0px;
	  -moz-border-radius-topleft: 0px;
	  -webkit-border-top-left-radius: 0px;
	  background: #edf1f2; /* Old browsers */
	  /* IE9 SVG, needs conditional override of 'filter' to 'none' */
	  background: url(data:image/svg+xml;base64,);
	  background: -moz-linear-gradient(-45deg,  #edf1f2 19%, #e5e9ea 100%); /* FF3.6+ */
	  background: -webkit-gradient(linear, left top, right bottom, color-stop(19%,#edf1f2), color-stop(100%,#e5e9ea)); /* Chrome,Safari4+ */
	  background: -webkit-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* Chrome10+,Safari5.1+ */
	  background: -o-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* Opera 11.10+ */
	  background: -ms-linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* IE10+ */
	  background: linear-gradient(-45deg,  #edf1f2 19%,#e5e9ea 100%); /* W3C */
	  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#edf1f2', endColorstr='#e5e9ea',GradientType=1 ); /* IE6-8 fallback on horizontal gradient */
}
ul, ol {
  list-style-position: inside;
}
ul.indent{
