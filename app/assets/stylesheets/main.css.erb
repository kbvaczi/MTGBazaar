.button, input[type="submit"] {
  -moz-box-shadow:    0 0 0 0 #FFF;
  -webkit-box-shadow: 0 0 0 0 #FFF;
  box-shadow:         0 0 0 0 #FFF;
  padding: 4px 7px 4px 7px;
  margin:0;
  color: #fff;
  display: inline-block;
  text-decoration: none;
  text-shadow: 0 -1px 0px rgb(100,100,100);
  text-align: center;
  background: #ABB0B1;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  background: rgb(192,192,192); /* Old browsers */
  background: -moz-linear-gradient(top,  rgba(192,192,192,1) 0%, rgba(176,176,176,1) 50%, rgba(159,159,159,1) 51%, rgba(112,112,112,1) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(192,192,192,1)), color-stop(50%,rgba(176,176,176,1)), color-stop(51%,rgba(159,159,159,1)), color-stop(100%,rgba(112,112,112,1))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top,  rgba(192,192,192,1) 0%,rgba(176,176,176,1) 50%,rgba(159,159,159,1) 51%,rgba(112,112,112,1) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top,  rgba(192,192,192,1) 0%,rgba(176,176,176,1) 50%,rgba(159,159,159,1) 51%,rgba(112,112,112,1) 100%); /* Opera 11.10+ */
  background: -ms-linear-gradient(top,  rgba(192,192,192,1) 0%,rgba(176,176,176,1) 50%,rgba(159,159,159,1) 51%,rgba(112,112,112,1) 100%); /* IE10+ */
  background: linear-gradient(top,  rgba(192,192,192,1) 0%,rgba(176,176,176,1) 50%,rgba(159,159,159,1) 51%,rgba(112,112,112,1) 100%); /* W3C */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#c0c0c0', endColorstr='#707070',GradientType=0 ); /* IE6-9 */
}
