  margin-left:20px;
}
p {
  word-wrap: break-word;
}
a {
  color: #688AC1;
	text-decoration: underline;
  text-shadow: none;
}
a:hover, a:focus {
	text-decoration: none;
}
select, input, input[type="text"], input[type="search"], textarea  {
  outline:none;
  border: 1px solid #aaa;
  padding: 3px 4px;
  -moz-border-radius: 8px;
  -webkit-border-radius: 8px;
  border-radius: 8px;
  -moz-box-shadow:    inset 0px 1px 4px 1px #ddd;
  -webkit-box-shadow: inset 0px 1px 4px 1px #ddd;
  box-shadow:         0px -1px 0px 0px #999 , inset 0px 1px 4px 0px #ddd;
}
select[disabled] {
 color: #bbb;
}

/* Change placeholder text color */
::-webkit-input-placeholder {
  color:    #bbb;
  font-style:italic;
}
:-moz-placeholder {
  color:    #bbb;
  font-style:italic;    
}
:-ms-input-placeholder {
  color:    #bbb;
  font-style:italic;    
}

/* clearfix */
.group:before, .group:after {
  content:"";
  display:table;
}
.group:after {
  clear:both;
}
.group {
  zoom:1; /* For IE 6/7 (trigger hasLayout) */
}
