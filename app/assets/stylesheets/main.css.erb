#right_bar, #right_bar2, #right_bar3 {
  float:left;
  margin: 10px 0px 0px 0;
  width: 230px;
  min-height: 195px;
  background: #FFF;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  -moz-box-shadow:    0px 1px 1px 0px #bbb;
  -webkit-box-shadow: 0px 1px 1px 0px #bbb;
  box-shadow:         0px 1px 1px 0px #bbb;
}
#center_bar {
  float:left;
  min-height:605px;
  margin: 10px;
  width: 540px;
  background: #FFF;
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
  -moz-box-shadow:    0px 1px 1px 0px #bbb;
  -webkit-box-shadow: 0px 1px 1px 0px #bbb;
  box-shadow:         0px 1px 1px 0px #bbb;  
}
#cta {
  background: #FFF;
  border-radius: 10px;
  z-index: 100;
}
#cta img {
  height: 250px;
  width: 300px;
  z-index: 1;
}
.sentry {
  margin: 10px auto;
  z-index: 0;
}

#bottomnav {
  padding: 5px 0;
  font-size: 11px;
  text-align: center;
}
#footer {
  padding: 5px 0;
  color: #58595B;
  font-size: 11px;
  text-align: center;
}
.noshow {
  float:right;
  font-size:.001px;
  color:transparent;
  display:inline-block;
  width: 0;
}

#friendly_captcha, .oiuk {
  position: absolute; 
  left: -999em;
}
.section {
  margin: 24px 10px 0 10px;
}
.section_heading {
  text-decoration: underline;
  margin: 6px auto;
}

/*------------------------------------*/
/*--generic content classes ----------*/
/*------------------------------------*/
.content {
  margin:15px;
}
.breadcrumb {
  margin-top: 10px;
}

.content_teaser {
  color: #333;

  background: rgb(240,240,240); /* Old browsers */
  background: -moz-linear-gradient(top, rgba(240,240,240,1) 0%, rgba(227,227,227,1) 100%); /* FF3.6+ */
  background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,rgba(240,240,240,1)), color-stop(100%,rgba(227,227,227,1))); /* Chrome,Safari4+ */
  background: -webkit-linear-gradient(top, rgba(240,240,240,1) 0%,rgba(227,227,227,1) 100%); /* Chrome10+,Safari5.1+ */
  background: -o-linear-gradient(top, rgba(240,240,240,1) 0%,rgba(227,227,227,1) 100%); /* Opera11.10+ */
  background: -ms-linear-gradient(top, rgba(240,240,240,1) 0%,rgba(227,227,227,1) 100%); /* IE10+ */
  filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#f0f0f0', endColorstr='#e3e3e3',GradientType=0 ); /* IE6-9 */
  background: linear-gradient(top, rgba(240,240,240,1) 0%,rgba(227,227,227,1) 100%); /* W3C */

  padding: 5px;
  margin-bottom: 5px;
  padding-left: 10px;
  padding-right: 10px;
  display:block;

  height: auto;
  -moz-border-radius: 5px;
  -webkit-border-radius: 5px;
  border-radius: 5px;
}
.content_teaser_top {
  height: 25px;
  line-height: 20px;
  border-bottom: 1px solid #BBB;
}
.content_teaser_name {
  display: inline;
  float: left;
  width: 400px;
  white-space: nowrap;
  overflow: hidden;
  line-height: 20px;  
}

.content_teaser_bottom {
  margin-top: 5px;
  display: block;
}

/*------------------------------------*/
/*-------------ToolTips---------------*/
/*------------------------------------*/
.tooltip {
  position: absolute;
  display: none;
  margin-left:5px;
  padding:10px; 
  margin-left:5px; 
  background:#999; 
  color:#FFF;   
  text-align:center;
  -moz-border-radius: 10px 10px 10px 0; 
  -webkit-border-radius: 10px 10px 10px 0; 
  border-radius: 10px 10px 10px 0;  
  -moz-box-shadow: 0px 0px 2px #444; 
  -webkit-box-shadow: 0px 0px 2px #444;
  box-shadow: 0px 0px 2px #444;
  z-index: 10;
}

/*------------------------------------*/
/*-------------Helpers----------------*/
/*------------------------------------*/
.spacer { clear: both; }
.left { display:inline-block; float: left; }
.right { display:inline-block; float: right; }
.center { min-width: 30%; } /* objects automatically centered using jquery */
.hidden { visibility:hidden; }
.mr {margin-right:15px; }
.mr-s {margin-right:5px; }
.mr-m {margin-right:10px; }
.ml {margin-left:15px; }
.ml-s {margin-left:5px; }
.ml-m {margin-left:10px; }
.mt {margin-top:15px; }
.mt-s {margin-top:5px; }
.mt-m {margin-top:10px; }
.mb {margin-bottom:15px; }
.mb-s {margin-bottom:5px; }
.mb-m {margin-bottom:10px; }
.ma {margin: 15px; }
.ma-s {margin: 5px; } /* margin-all (small) */
.ma-m {margin: 10px; }
.t-s {font-size: 70%; line-height:2.9;}

/* root element for tabs  */
.tabs { 
	list-style:none; 
	margin:10px 3px 0 3px; 
	display:inline-block;
}

.tabs li { 
	float:left;
  text-indent:0;
	list-style: none;
}

.tabs li a { 
	display:block;
	text-align:center;	
	text-decoration:none;
	color:#444;
	padding: 3px 15px 3px 15px;
}

.tabs .current a, .tabs li a:hover{
  background-color: rgb(237,241,242);
  -moz-border-radius: 10px;
  -webkit-border-radius: 10px;
  border-radius: 10px;
}