  background-image: -webkit-linear-gradient(-45deg,  rgba(226,226,226,0.1) 0%,rgba(219,219,219,0.2) 50%,rgba(209,209,209,0.2) 51%,rgba(254,254,254,0.1) 100%);
  background-image: -o-linear-gradient(-45deg,  rgba(226,226,226,0.1) 0%,rgba(219,219,219,0.2) 50%,rgba(209,209,209,0.2) 51%,rgba(254,254,254,0.1) 100%);
  background-image: -ms-linear-gradient(-45deg,  rgba(226,226,226,0.1) 0%,rgba(219,219,219,0.2) 50%,rgba(209,209,209,0.2) 51%,rgba(254,254,254,0.1) 100%);
  background-image: linear-gradient(135deg,  rgba(226,226,226,0.1) 0%,rgba(219,219,219,0.2) 50%,rgba(209,209,209,0.2) 51%,rgba(254,254,254,0.1) 100%);
  overflow:hidden;
  padding:10px;
}
