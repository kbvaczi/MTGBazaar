.decklist_wrapper .decklist_title {
  margin:-10px;
  box-shadow:0px 0px 1px 0px #EEE;  
  background: -moz-linear-gradient(-45deg,  rgba(104,138,193,0.07) 0%, rgba(0,0,0,0) 100%);
  background: -webkit-gradient(linear, left top, right bottom, color-stop(0%,rgba(104,138,193,0.07)), color-stop(100%,rgba(0,0,0,0)));
  background: -webkit-linear-gradient(-45deg,  rgba(104,138,193,0.07) 0%,rgba(0,0,0,0) 100%);
  background: -o-linear-gradient(-45deg,  rgba(104,138,193,0.07) 0%,rgba(0,0,0,0) 100%);
  background: -ms-linear-gradient(-45deg,  rgba(104,138,193,0.07) 0%,rgba(0,0,0,0) 100%);
  background: linear-gradient(135deg,  rgba(104,138,193,0.07) 0%,rgba(0,0,0,0) 100%);
}
.decklist_wrapper .decklist_title h2 {
  border:none;
  box-shadow:none;
  margin:0px 0px 10px 0px;
  padding:5px 0px 5px 15px;
  font-size:13px;
}
.decklist_wrapper .decklist_title h2 a {
  margin-top:2px;
  font-weight:normal; 
}
.decklist_wrapper .decklist_image_wrapper {
  text-align:center;
  display:inline-block;
  margin-top:15px;
  padding:0px;
  background:#000;
  border-radius:12px;
  overflow:hidden;
  background-image: url(https://mtgbazaar.s3.amazonaws.com/images/mtg/logo/logo_card_sleeve.jpg);
}
.decklist_wrapper .decklist_image_wrapper .decklist_image {
  height:310px;
  width:223px;
  border-radius:12px;  
  padding:0px;
  margin:0px;
  background-size:100%;
  background-repeat:none;
  box-shadow: inset 0px 0px 2px 1px #000;     
}
.decklist_image_wrapper .decklist_image .decklist_image_shadow {

}
.decklist_wrapper .decklist_content {
  vertical-align:top;  
  width:53%;
  padding:0px;
  margin:0px;
  margin-top:5px;
  display:inline-block;
  white-space:normal;
}
.decklist_wrapper .decklist_content .decklist_section {
  padding-right:5px;
}
.decklist_wrapper .decklist_section .decklist_section_title {
  padding-left:10px;
  margin-left:-5px;
  margin-right:5px;
  text-align:left;
  border:none;
  background: -moz-linear-gradient(left,  rgba(0,0,0,00.04) 0%, rgba(0,0,0,0.01) 100%);
  background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(0,0,0,00.04)), color-stop(100%,rgba(0,0,0,0.01)));
  background: -webkit-linear-gradient(left,  rgba(0,0,0,00.04) 0%,rgba(0,0,0,0.01) 100%);
  background: -o-linear-gradient(left,  rgba(0,0,0,00.04) 0%,rgba(0,0,0,0.01) 100%);
  background: -ms-linear-gradient(left,  rgba(0,0,0,00.04) 0%,rgba(0,0,0,0.01) 100%);
  background: linear-gradient(to right,  rgba(0,0,0,00.04) 0%,rgba(0,0,0,0.01) 100%);
  border-radius: 10px; 
}
.decklist_section_title h3 {
  text-decoration:none;
  padding:3px 0px 1px 0px;
}
.decklist_section .decklist_subsection {
  white-space:normal;  
}
.decklist_subsection .decklist_subsection_title {
  text-align:center;
  margin-right:20px;
}
.decklist_subsection .decklist_subsection_title h3 {
  font-size:12px;
  text-decoration:none;
  margin-bottom:5px;
  padding-bottom:0px;  
  margin-top:5px;
}
.decklist_subsection .decklist_card {
  display:inline-block;
  text-align:center;  
}
.decklist_card .card_quantity {
  display:inline-block;
  width:15px;
  padding-right:5px;
  text-align:right;
  color:#777;
}
.decklist_card .card_name_wrapper {
  display:inline-block;
  width:120px;
  height:15px;
  overflow:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align:left;
}