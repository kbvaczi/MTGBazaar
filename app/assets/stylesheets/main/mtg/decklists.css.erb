.decklist_package {
  line-height:16px;
  position:relative;
  margin-top:15px;
  margin-bottom:15px;  
  text-align:center;
}
.decklist_wrapper {
  width:470px;
  display:inline-block; 
  margin-left:auto;
  margin-right:auto;  
  position:relative;
  z-index:2;
  text-align:left;  
  border-radius: 10px;
  border:1px solid #EEE;
  background: rgb(255,255,255);
  background-image: -moz-linear-gradient(-45deg,  rgba(226,226,226,0.1) 0%, rgba(219,219,219,0.1) 50%, rgba(209,209,209,0.1) 51%, rgba(254,254,254,0.1) 100%);
  background-image: -webkit-gradient(linear, left top, right bottom, color-stop(0%,rgba(226,226,226,0.1)), color-stop(50%,rgba(219,219,219,0.1)), color-stop(51%,rgba(209,209,209,0.1)), color-stop(100%,rgba(254,254,254,0.1)));
  background-image: -webkit-linear-gradient(-45deg,  rgba(226,226,226,0.1) 0%,rgba(219,219,219,0.1) 50%,rgba(209,209,209,0.1) 51%,rgba(254,254,254,0.1) 100%);
  background-image: -o-linear-gradient(-45deg,  rgba(226,226,226,0.1) 0%,rgba(219,219,219,0.1) 50%,rgba(209,209,209,0.1) 51%,rgba(254,254,254,0.1) 100%);
  background-image: -ms-linear-gradient(-45deg,  rgba(226,226,226,0.1) 0%,rgba(219,219,219,0.1) 50%,rgba(209,209,209,0.1) 51%,rgba(254,254,254,0.1) 100%);
  background-image: linear-gradient(135deg,  rgba(226,226,226,0.1) 0%,rgba(219,219,219,0.1) 50%,rgba(209,209,209,0.1) 51%,rgba(254,254,254,0.1) 100%);
  padding:10px;
  vertical-align:bottom;
}
.decklist_wrapper .decklist_title {
  margin:-10px;
  box-shadow: inset 0px 0px 10px 0px #FFF;  
  background: -moz-linear-gradient(-45deg,  rgba(0,0,0,0.01) 0%, rgba(0,0,0,0.04) 100%);
  background: -webkit-gradient(linear, left top, right bottom, color-stop(0%,rgba(0,0,0,0.01)), color-stop(100%,rgba(0,0,0,0.04)));
  background: -webkit-linear-gradient(-45deg,  rgba(0,0,0,0.01) 0%,rgba(0,0,0,0.04) 100%);
  background: -o-linear-gradient(-45deg,  rgba(0,0,0,0.01) 0%,rgba(0,0,0,0.04) 100%);
  background: -ms-linear-gradient(-45deg,  rgba(0,0,0,0.01) 0%,rgba(0,0,0,0.04) 100%);
  background: linear-gradient(135deg,  rgba(0,0,0,0.01) 0%,rgba(0,0,0,0.04) 100%);
}
.decklist_wrapper .decklist_title h2 {
  border:none;
  box-shadow:none;
  margin:0px 0px 10px 0px;
  padding:5px 0px 5px 15px;
}
.decklist_wrapper .decklist_title h2 a {
  margin-top:2px;
  font-weight:normal; 
}

.decklist_wrapper .decklist_image_wrapper {
  text-align:center;
  display:inline-block;
  margin-top:15px;
  padding:0px;
  border-radius:12px;
}
.decklist_wrapper .decklist_image_wrapper .decklist_image {
  height:250px;
  width:180px;
  border-radius:12px;  
  padding:0px;
  margin:0px;
  background-size:100%;
  background-repeat: no-repeat;
  box-shadow: inset 0px 0px 2px 1px #000;  
  cursor:-webkit-zoom-in;
  cursor:-moz-zoom-in;
  background-color:#000;
  background-image: url(https://mtgbazaar.s3.amazonaws.com/images/mtg/logo/logo_card_sleeve.jpg);
}
.decklist_image_wrapper .decklist_image:active{
  position:relative;
  z-index:5;
  transform: scale(1.4);
  -ms-transform: scale(1.4); /* IE 9 */
  -webkit-transform: scale(1.4); /* Safari and Chrome */
  -o-transform: scale(1.4); /* Opera */
  -moz-transform: scale(1.4); /* Firefox */
  transition-duration: 0.3s;
  -moz-transition-duration: 0.3s;
  -webkit-transition-duration: 0.3s;
  -o-transition-duration: 0.3s;  
}
.decklist_image_wrapper .decklist_card_sales_frame {
    display:inline-block;
    width:150px;
    height:50px;
    padding:0px;
    margin:10px 0px;
}
.decklist_wrapper .decklist_content {
  vertical-align:top;  
  width:280px;
  padding:0px;
  margin:0px;
  margin-top:5px;
  display:inline-block;
  white-space:normal;
}
.decklist_wrapper .decklist_content .decklist_section {

}
.decklist_wrapper .decklist_section .decklist_section_title {
  padding-left:10px;
  border-radius:4px;
  margin-right:5px;
  text-align:left;
  border:none;
  box-shadow: inset 0px 2px 10px 0px #FFF;      
  background: -moz-linear-gradient(45deg,  rgba(0,0,0,0.02) 0%, rgba(0,0,0,0) 100%);
  background: -webkit-gradient(linear, left bottom, right top, color-stop(0%,rgba(0,0,0,0.02)), color-stop(100%,rgba(0,0,0,0)));
  background: -webkit-linear-gradient(45deg,  rgba(0,0,0,0.02) 0%,rgba(0,0,0,0) 100%);
  background: -o-linear-gradient(45deg,  rgba(0,0,0,0.02) 0%,rgba(0,0,0,0) 100%);
  background: -ms-linear-gradient(45deg,  rgba(0,0,0,0.02) 0%,rgba(0,0,0,0) 100%);
  background: linear-gradient(45deg,  rgba(0,0,0,0.02) 0%,rgba(0,0,0,0) 100%);
}
.decklist_section_title h3 {
  color:#777;
  text-decoration:none;
  margin: 5px;
  padding:3px 0px 1px 0px;
}
.decklist_section .decklist_subsection {
  white-space:normal;  
  margin-bottom:10px;
}
.decklist_subsection .decklist_subsection_title {
  text-align:center;
  margin-right:10px;
}
.decklist_subsection .decklist_subsection_title h3 {
  color:#888;
  text-shadow: none;
  font-weight:normal;
  font-size:12px;
  text-decoration:none;
  margin-bottom:5px;
  padding-bottom:0px;  
  margin-top:5px;
}
.decklist_subsection .decklist_card {
  display:inline-block;
  text-align:center;  
}
.decklist_card .card_quantity {
  display:inline-block;
  width:15px;
  padding-right:5px;
  text-align:right;
  color:#777;
}
.decklist_card .card_name_wrapper {
  display:inline-block;
  width:135px;
  height:15px;
  overflow:hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  text-align:left;
}